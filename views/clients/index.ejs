            <% if (alert.length > 0) { %>
            <div class="alert alert-success" role="alert">
                <%= alert %>
            </div>
            <% } %>
            <div class="table-responsive">
                <table class="table table-striped table-sm">
                    <thead>
                    <tr>
                        <th>#</th>
                        <th>First name</th>
                        <th>Last name</th>
                        <th>DOB</th>
                        <th>Gender</th>
                        <th>City</th>
                        <th>State</th>
                        <th>Email</th>
                        <th>Phone</th>
                        <% if (req.user.username === 'admin') { %>
                            <th>Action</th>
                            <th>Action</th>
                        <% } %>
                    </tr>
                    </thead>
                    <tbody>
                    <% for(var i = 0; i < clients.length; i++) { %>
                        <tr>
                            <td><%= clients[i].id %></td>
                            <td><%= clients[i].firstName %></td>
                            <td><%= clients[i].lastName %></td>
                            <td><%= clients[i].dob %></td>
                            <td><%= clients[i].gender %></td>
                            <td><%= clients[i].city %></td>
                            <td><%= clients[i].state %></td>
                            <td><%= clients[i].email %></td>
                            <td><%= clients[i].phone %></td>
                            <% if (req.user.username === 'admin') { %>
                                <td><a class="btn btn-outline-secondary" href="/clients/<%= clients[i].id %>/edit">Edit</a></td>
                                <td>
                                    <form action="/clients/<%= clients[i].id %>?_method=DELETE" method="post">
                                        <button type="submit" class="btn btn-outline-secondary">Delete</button>
                                    </form>
                                </td>
                            <% } %>
                        </tr>

                    <% } %>
                    </tbody>
